<%= render 'users/user info' %>
<%= render 'books/new book', book_new: @book_new %>

<h2>Users</h2>
    <table class="table">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td>
            <p><%= attachment_image_tag user, :profile_image, :fill, 40, 40, format: 'jpeg', fallback: "no_image.jpg" %></p>
          </td>
          <td>
            <p><%= user.name %></p>
          </td>
          <td>
            <p><%= link_to"Show",user_path(user.id) %></p>
          </td>
        </tr>
        <% end %>
      </tbody>
    </table>